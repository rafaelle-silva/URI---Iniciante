a, b, c = input().split()

a = float(a)
b = float(b)
c = float(c)

areaT = (a * c) / 2
areaC =  3.14159 * (c**2)
cont1 = (a + b) * c 
areaTP = cont1 / 2
areaQ = b * b
areaR = a * b 

print('TRIANGULO: {:.3f}' .format(areaT))
print('CIRCULO: {:.3f}' .format(areaC))
print('TRAPEZIO: {:.3f}' .format(areaTP))
print('QUADRADO: {:.3f}' .format(areaQ))
print('RETANGULO: {:.3f}' .format(areaR))
